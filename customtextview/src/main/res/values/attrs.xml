<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CustomTextView">

        <attr name="android:padding" />
        <attr name="android:paddingLeft" />
        <attr name="android:paddingRight" />
        <attr name="android:paddingTop" />
        <attr name="android:paddingBottom" />
        <attr name="android:onClick" />
        <attr name="lib_setRadius" format="dimension" />
        <attr name="lib_setRoundedView" format="boolean" />
        <attr name="lib_setRoundedBorderColor" format="color" />
        <attr name="lib_setRoundedBGColor" format="color" />
        <attr name="lib_setStrokeWidth" format="dimension" />
        <attr name="lib_setFont" format="string" />
        <attr name="lib_setShape" format="enum">
            <enum name="rectangle" value="0" />
            <enum name="oval" value="1" />
        </attr>
        <attr name="lib_setUnderLineText" format="boolean" />
        <attr name="lib_setStrikeText" format="boolean" />
        <attr name="lib_setExpandableText" format="boolean" />
        <attr name="lib_setActionTextColor" format="color" />
        <attr name="lib_setActionTextVisible" format="boolean" />
        <attr name="lib_setTrimLines" format="integer" />
        <attr name="lib_setCheckedText" format="boolean" />
        <attr name="lib_setChecked" format="boolean" />
        <attr name="lib_checkedDrawable" format="reference" />
        <attr name="lib_unCheckedDrawable" format="reference" />
        <attr name="lib_checkMarkTint" format="reference" />
        <attr name="lib_checkedIconPosition" format="enum">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
        </attr>
        <attr name="lib_checkedDrawablePadding" format="dimension" />
        <attr name="lib_setExpandHint" format="string" />
        <attr name="lib_setCollapseHint" format="string" />

    </declare-styleable>

</resources>